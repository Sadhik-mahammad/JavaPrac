class Box
{
int width,height,depth;
void volume(int w,int d, int h){
this.width=w;
this.height=h;
this.depth=d;
int vol=w*h*d;
System.out.println("This is a box");
System.out.println("this is the volume of the box:"+vol);
}
public static void main(String args[])
{
Box bo=new Box();
bo.volume(2,3,4);
}
}
